{
  "name": "terminal-jarvis",
  "version": "0.0.8",
  "description": "A thin Rust wrapper that provides a unified interface for managing and running AI coding tools",
  "bin": "lib/index.js",
  "files": [
    "lib/",
    "README.md",
    "package.json"
  ],
  "scripts": {
    "sync-readme": "cp ../../README.md .",
    "typecheck": "tsc --noEmit",
    "lint": "biome lint src/",
    "lint:fix": "biome lint --write src/",
    "format": "biome format src/",
    "format:fix": "biome format --write src/",
    "check": "biome check src/",
    "check:fix": "biome check --write src/",
    "build": "npm run sync-readme && tsc",
    "prepublishOnly": "npm run build",
    "dev": "npm run build && node lib/index.js",
    "postinstall": "node -e \"console.log('\\nðŸ¤– Terminal Jarvis installed!\\n\\nFor the full T.JARVIS experience, install the Rust binary:\\n  cargo install --git https://github.com/BA-CalderonMorales/terminal-jarvis\\n\\nThen run: npx terminal-jarvis\\n')\""
  },
  "keywords": [
    "cli",
    "ai",
    "tools",
    "wrapper",
    "claude",
    "gemini",
    "qwen",
    "opencode"
  ],
  "author": "BA-CalderonMorales",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/BA-CalderonMorales/terminal-jarvis.git"
  },
  "homepage": "https://github.com/BA-CalderonMorales/terminal-jarvis#readme",
  "devDependencies": {
    "@biomejs/biome": "^2.1.3",
    "@types/node": "^20.0.0",
    "typescript": "^5.0.0"
  },
  "engines": {
    "node": ">=16.0.0"
  }
}
