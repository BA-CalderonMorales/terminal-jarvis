{
  "name": "terminal-jarvis",
  "version": "0.0.11",
  "description": "A thin Rust wrapper that provides a unified interface for managing and running AI coding tools",
  "bin": "lib/index.js",
  "files": [
    "lib/",
    "bin/",
    "README.md",
    "package.json"
  ],
  "scripts": {
    "sync-readme": "cp ../../README.md .",
    "build-rust": "cd ../.. && cargo build --release",
    "copy-binary": "node build-binaries.js",
    "typecheck": "tsc --noEmit",
    "lint": "biome lint src/",
    "lint:fix": "biome lint --write src/",
    "format": "biome format src/",
    "format:fix": "biome format --write src/",
    "check": "biome check src/",
    "check:fix": "biome check --write src/",
    "build": "npm run sync-readme && npm run copy-binary && tsc",
    "prepublishOnly": "npm run build",
    "dev": "npm run build && node lib/index.js",
    "postinstall": "node -e \"console.log('\\nðŸ¤– Terminal Jarvis v0.0.11 installed successfully!\\n\\nâœ… Full T.JARVIS interface ready to use!\\n\\nRun: npx terminal-jarvis\\n')\""
  },
  "keywords": [
    "cli",
    "ai",
    "tools",
    "wrapper",
    "claude",
    "gemini",
    "qwen",
    "opencode"
  ],
  "author": "BA-CalderonMorales",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/BA-CalderonMorales/terminal-jarvis.git"
  },
  "homepage": "https://github.com/BA-CalderonMorales/terminal-jarvis#readme",
  "devDependencies": {
    "@biomejs/biome": "^2.1.3",
    "@types/node": "^20.0.0",
    "typescript": "^5.0.0"
  },
  "engines": {
    "node": ">=16.0.0"
  }
}
